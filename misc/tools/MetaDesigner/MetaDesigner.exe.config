      <add type="collection" targetType="Entity" name="BatchGenerateXmlMapping" title="NHibernate Mappings" parent="Generate" handler="DataGeneration.Engine.GenerateFileBatchCommand,BabakSoft.Tools.MetaDesigner.Engine">
