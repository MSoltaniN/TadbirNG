      <add type="collection" targetType="Entity" name="BatchGenerateMappingTestClass" title="Mapping Test Classes" parent="Generate" handler="DataGeneration.Engine.GenerateFileBatchCommand,BabakSoft.Tools.MetaDesigner.Engine">
