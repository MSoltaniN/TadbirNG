        </parameters>
      </add>
    </commands>
    <defaultCommands>
      <add type="collection" targetType="" name="NewItem" title="New {0}" parent="" needsInput="true" handler="DataGeneration.Engine.AddItemCommand,BabakSoft.Tools.MetaDesigner.Engine">
        <parameters>
          <add name="collection" type="" value="" />
          <add name="wrapper" type="" value="" />
        </parameters>
      </add>
      <add type="item" targetType="" name="DeleteItem" title="Delete" parent="" handler="DataGeneration.Engine.DeleteItemCommand,BabakSoft.Tools.MetaDesigner.Engine">
        <parameters>
          <add name="item" type="" value="" />
          <add name="collection" type="" value="" />
          <add name="wrapper" type="" value="" />
        </parameters>
      </add>
    </defaultCommands>
    <editors>
      <add name="EntityEditor" type="DataGeneration.Forms.NewEntity,BabakSoft.Tools.MetaDesigner.Forms" itemType="BabakSoft.Platform.Metadata.Entity,BabakSoft.Platform.Metadata" />
      <add name="PropertyEditor" type="DataGeneration.Forms.NewProperty,BabakSoft.Tools.MetaDesigner.Forms" itemType="BabakSoft.Platform.Metadata.Property,BabakSoft.Platform.Metadata" />
      <add name="RelationEditor" type="DataGeneration.Forms.NewRelation,BabakSoft.Tools.MetaDesigner.Forms" itemType="BabakSoft.Platform.Metadata.Relation,BabakSoft.Platform.Metadata" />
    </editors>
