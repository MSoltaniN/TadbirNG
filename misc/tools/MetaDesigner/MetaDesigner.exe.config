        <parameters>
          <add name="item" type="Entity" value="" />
          <add name="fileName" type="System.String" value="{0}Map.Generated.cs" />
          <add name="template" type="System.String" value="BabakSoft.Tools.MetaDesigner.Transforms.FluentMappingFromMetadata,BabakSoft.Tools.MetaDesigner.Transforms" />
        </parameters>
      </add>
      <add type="item" targetType="Entity" name="GenerateMappingTest" title="Mapping Test Class" parent="Generate" handler="DataGeneration.Engine.GenerateFileCommand,BabakSoft.Tools.MetaDesigner.Engine">
        <parameters>
          <add name="item" type="Entity" value="" />
