          <add name="fileName" type="System.String" value="{0}Repository.Generated.cs" />
          <add name="template" type="System.String" value="BabakSoft.Tools.MetaDesigner.Transforms.NHibernateRepository,BabakSoft.Tools.MetaDesigner.Transforms" />
        </parameters>
      </add>
      <add type="collection" targetType="Entity" name="BatchGeneratePocoClass" title="POCO Classes" parent="Generate" handler="DataGeneration.Engine.GenerateFileBatchCommand,BabakSoft.Tools.MetaDesigner.Engine">
        <parameters>
          <add name="fileName" type="System.String" value="{0}.Generated.cs" />
          <add name="template" type="System.String" value="BabakSoft.Tools.MetaDesigner.Transforms.PocoFromMetadata,BabakSoft.Tools.MetaDesigner.Transforms" />
