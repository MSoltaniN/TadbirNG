          <add name="fileName" type="System.String" value="{0}.hbm.xml" />
          <add name="template" type="System.String" value="BabakSoft.Tools.MetaDesigner.Transforms.MappingFromMetadata,BabakSoft.Tools.MetaDesigner.Transforms" />
        </parameters>
      </add>
      <add type="collection" targetType="Entity" name="BatchGenerateCreateTable" title="CREATE TABLE Scripts" parent="Generate" handler="DataGeneration.Engine.GenerateFileBatchCommand,BabakSoft.Tools.MetaDesigner.Engine">
        <parameters>
          <add name="fileName" type="System.String" value="{0}_CreateTable.sql" />
          <add name="template" type="System.String" value="BabakSoft.Tools.MetaDesigner.Transforms.TableFromMetadata,BabakSoft.Tools.MetaDesigner.Transforms" />
