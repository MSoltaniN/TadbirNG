<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="babaksoft.platform.metaDesigner" type="BabakSoft.Platform.Configuration.MetaDesignerSection, BabakSoft.Platform.Configuration"/>
  </configSections>
  <babaksoft.platform.metaDesigner>
    <generators>
      <add name="PocoGenerator" outputType="Poco" fileName="{0}.Generated.cs" template="DataGeneration.Engine.PocoFromMetadata, BabakSoft.Tools.MetaDesigner.Engine" />
      <add name="ViewModelGenerator" outputType="ViewModel" fileName="{0}ViewModel.Generated.cs" template="DataGeneration.Engine.ViewModelFromMetadata, BabakSoft.Tools.MetaDesigner.Engine" />
      <add name="FluentMappingGenerator" outputType="FluentMapping" fileName="{0}Map.Generated.cs" template="DataGeneration.Engine.FluentMappingFromMetadata, BabakSoft.Tools.MetaDesigner.Engine" />
      <add name="MappingTestGenerator" outputType="MappingTest" fileName="{0}MapTest.Generated.cs" template="DataGeneration.Engine.MappingTestFromMetadata, BabakSoft.Tools.MetaDesigner.Engine" />
      <add name="MappingGenerator" outputType="XmlMapping" fileName="{0}.hbm.xml" template="DataGeneration.Engine.MappingFromMetadata, BabakSoft.Tools.MetaDesigner.Engine" />
      <add name="TableGenerator" outputType="CreateTable" fileName="{0}_CreateTable.sql" template="DataGeneration.Engine.TableFromMetadata, BabakSoft.Tools.MetaDesigner.Engine" />
      <add name="RepositoryGenerator" outputType="Repository" fileName="{0}Repository.Generated.cs" template="DataGeneration.Engine.NHibernateRepository, BabakSoft.Tools.MetaDesigner.Engine" />
      <add name="DummyGenerator" outputType="DummyFactory" fileName="DummyFactory.Generated.cs" template="DataGeneration.Engine.DummyFactoryFromMetadata, BabakSoft.Tools.MetaDesigner.Engine" />
    </generators>
    <commands>
      <add type="object" targetType="Repository" name="GenerateDummyFactoryClass" title="DummyFactory Class" parent="Generate" handler="DataGeneration.Engine.GenerateFileCommand,BabakSoft.Tools.MetaDesigner.Engine">
        <parameters>
          <add name="object" type="Repository" value="" />
          <add name="fileName" type="System.String" value="DummyFactory.Generated.cs" />
          <add name="template" type="System.String" value="BabakSoft.Tools.MetaDesigner.Transforms.DummyFactoryFromMetadata,BabakSoft.Tools.MetaDesigner.Transforms" />
        </parameters>
      </add>
      <add type="item" targetType="Entity" name="GeneratePocoClass" title="POCO Class" parent="Generate" handler="DataGeneration.Engine.GenerateFileCommand,BabakSoft.Tools.MetaDesigner.Engine">
        <parameters>
          <add name="item" type="Entity" value="" />
          <add name="fileName" type="System.String" value="{0}.Generated.cs" />
          <add name="template" type="System.String" value="BabakSoft.Tools.MetaDesigner.Transforms.PocoFromMetadata,BabakSoft.Tools.MetaDesigner.Transforms" />
        </parameters>
      </add>
      <add type="item" targetType="Entity" name="GenerateViewModelClass" title="View Model Class" parent="Generate" handler="DataGeneration.Engine.GenerateFileCommand,BabakSoft.Tools.MetaDesigner.Engine">
